import java.util.Scanner;

public class Problem2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		Scanner sc = new Scanner(System.in);
		System.out.println("Please input size of the array.");
		
		int i = sc.nextInt(); //Size
		
		int [] Масив = new int [i]; 
		
		System.out.println("Please input the content of the array.");
		for (int a = 0; a < Масив.length; a++){
			Масив[a] = sc.nextInt();
		}
		
		int j = i * 2; //Size of the second array.
		
		int [] Масив2 = new int [j];
		
		for (int a = 0; a < Масив.length; a++){
			Масив2[a] = Масив[a];
			for (int b = 0; a < (j - 1) - i; b++){
				Масив2[b] = Масив[b];
			}
		}
		
		for (int a = 0; a < Масив2.length; a++){
		System.out.print(Масив2[a]+ ", ");
		}
	}

}
